def honi(x):
    honi = 0
    for i in range(honi):
        if x[i]=="H" == True:
            if x[i]=="O" == True:
                if x[i]=="N" == True:
                    if x[i]== "I" == True:
                        honi+=1
                        


print(honi("HHHHOOOONNNNIIII"))